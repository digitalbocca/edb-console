{
  "name": "edb-console",
  "version": "0.7.0",
  "description": "Habilita e desabilita mensagens do console. Ferramenta de desenvolvimento EDB.",
  "main": "src/",
  "config": {
    "commitMessage": "Atualização de dependências"
  },
  "scripts": {
    "git": "npm run gst && npm run gad && npm run gco && npm run gta && npm run gpu",
    "gst": "git status",
    "gad": "git add .",
    "gco": "cross-var git commit -m \"$npm_package_config_commitMessage\"",
    "gta": "cross-var git tag v$npm_package_version",
    "gpu": "npm run gpo && npm run gpb && npm run gpg",
    "gpo": "git push origin master --tags",
    "gpb": "git push bb master --tags",
    "gpg": "git push gl master --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/digitalbocca/edb-console.git"
  },
  "keywords": [
    "console",
    "log",
    "onoff"
  ],
  "author": "Gabriel Bertola Bocca - gabriel@estudiodigitalbocca.com.br",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/digitalbocca/edb-console/issues"
  },
  "homepage": "https://github.com/digitalbocca/edb-console#readme",
  "devDependencies": {
    "cross-var-no-babel": "^1.2.0",
    "standard": "^13.1.0"
  }
}
